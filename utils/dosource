#!/bin/bash
#
# DO NOT EDIT! Used by 'pybs' only.
#

main() {
    file=$1
    N=$2
    V=$3
    R=$4
    source $file
    echo "
DESCRIPTION="$DESCRIPTION"
COMMENTS="$COMMENTS"
HOMEPAGE="$HOMEPAGE"
LICENSE="$LICENSE"
PRIORITY="$PRIORITY"
PACKAGER="$PACKAGER"
GROUP="$GROUP"
YARCH="$YARCH"
REPO="$REPO"
SRC_URI="$SRC_URI"
CHECKSUM="$CHECKSUM"
RDEPEND="$RDEPEND"
BDEPEND="$RDEPEND $BDEPEND"
RECOMMENDED="$RECOMMENDED"
OPTIONAL="$OPTIONAL"
CONFLICT="$CONFLICT"
REPLACE="$REPLACE"
OPTIONS="$OPTIONS"
INSTALL="$INSTALL"
ALIAS="$ALIAS"
RIR="$RIR"
DESKTOPFILE="$DESKTOPFILE"
ICONFILE="$ICONFILE"
NOTES="$NOTES"
"
}

main "$@"
